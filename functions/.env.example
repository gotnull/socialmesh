# Firebase Cloud Functions Environment Variables
# Copy this file to .env and fill in your values
# See: https://firebase.google.com/docs/functions/config-env

# RevenueCat webhook secret for signature verification
# Get from RevenueCat Dashboard > Project Settings > Webhooks
REVENUECAT_WEBHOOK_SECRET=your_webhook_secret_here

# Admin API key for protected endpoints
# Generate a strong random string for production
ADMIN_KEY=your_admin_key_here

# Firebase Project Configuration
FIREBASE_PROJECT_ID=social-mesh-app

# App URLs
APP_BASE_URL=https://socialmesh.app
APP_STORE_URL=https://apps.apple.com/app/socialmesh/id6739187207
PLAY_STORE_URL=https://play.google.com/store/apps/details?id=app.socialmesh

# App Identifiers
IOS_APP_STORE_ID=6739187207
ANDROID_PACKAGE=app.socialmesh
